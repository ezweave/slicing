  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@types/async": "^3.2.15",
    "@types/aws-lambda": "^8.10.97",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.183",
    "@types/node": "^18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.42.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1136.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-functional": "^4.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-sonarjs": "^0.16.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "jest": "^27.5.1",
    "prettier": "^2.7.1",
    "serverless": "^3.25.0",
    "serverless-dotenv-plugin": "^4.0.1",
    "serverless-offline": "11.5.0",
    "serverless-plugin-datadog": "^5.15.0",
    "serverless-webpack": "^5.11.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "ts-prune": "^0.10.3",
    "tsconfig-paths-jest": "^0.0.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "tsutils": "^3.21.0",
    "typescript": "^4.5.5",
    "webpack": "^5.72.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "build": "yarn lint && tsc",
    "clean:reset": "rm -rf node_modules && yarn",
    "clean": "rm -rf dist && rm -rf .serverless",
    "deploy-local": "scripts/sls-deploy.sh",
    "deploy-serverless": "yarn serverless deploy",
    "find-unused": "ts-prune",
    "fix": "yarn lint:fix && yarn format:fix",
    "print-serverless": "DEPLOYMENT_REGION=local STAGE_NAME=offline ./node_modules/.bin/sls print",
    "format": "prettier --check ./src/**",
    "format:fix": "prettier --write ./src/**",
    "lint:fix": "eslint --fix './src/**/*.ts'",
    "lint": "eslint './src/**/*.ts'",
    "offline": "yarn build && DEPLOYMENT_REGION=local STAGE_NAME=offline node --max-old-space-size=4096 --inspect=7010 ./node_modules/.bin/sls offline --stage offline --httpPort 3010",
    "sls-lint": "STAGE_NAME=offline DEPLOYMENT_REGION=us-east-1 yarn sls print",
    "test": "jest -i",
    "test-noc": "jest -i --collectCoverage=false"
  }
}